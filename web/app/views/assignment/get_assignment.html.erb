<style>
body {
  max-width: 50%;
  margin-left: 2em;
}
</style>
<%= raw(@html) %>
<script>
$(function() {
  var repl = $("<div>").addClass("repl");;
  repl.css({height: "100%"});
  window.PANEL = createTabPanel($(document.body));
  PANEL.addTab("Interactions", repl, { cannotClose: true });
  window.REPL = repl;
  window.RUN_CODE = makeRepl(repl);
  ct_transform($(document.body));
});
</script>
